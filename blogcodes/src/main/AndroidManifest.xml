<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.zhy.blogcodes"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CategoryActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".nav.NavigationViewActivity"
            android:label="@string/title_activity_category"
            android:theme="@style/Theme.DesignDemo">
        </activity>
        <activity
            android:name=".nav.NavListViewActivity"
            android:label="@string/title_activity_base_content"
            android:theme="@style/Theme.DesignDemo">
        </activity>
        <activity android:name=".vdh.VDHBlogActivity"></activity>
        <activity
            android:name=".mvp.UserLoginActivity"
            android:label="@string/title_activity_user_login">
        </activity>
        <activity
            android:name=".dl.DLCategoryActivity"
            android:label="@string/title_activity_dlactivity01">
        </activity>
        <activity
            android:name=".dl.DLActivity01"
            android:label="@string/title_activity_dlactivity01">
        </activity>
        <activity
            android:name=".dl.DLProxyActivity"
            android:label="@string/title_activity_dlbase"
            android:theme="@android:style/Theme.Light">
        </activity>
    </application>

</manifest>
